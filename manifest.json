{
    "background": {
        "scripts": [ "background.js" ]
    },
    "browser_action": {
        "default_icon": {
            "16": "img/vomiting.png"
        }
    },
    "content_scripts": [{
        "all_frames": true,
        "js": [ "content.js" ],
        "matches": [ "*://*/*" ]
    }],
    "devtools_page": "index.html",
    "manifest_version": 2,
    "name": "Sample Extension",
    "version": "1.0.0",
    "web_accessible_resources": [
        "injected.js"
    ]
}
